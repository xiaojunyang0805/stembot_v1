{
  "name": "stembot_v1",
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "regions": ["iad1"],
  "functions": {
    "src/app/**/*.ts": {
      "maxDuration": 30,
      "memory": 1024
    },
    "src/app/**/*.tsx": {
      "maxDuration": 30,
      "memory": 1024
    }
  },
  "build": {
    "env": {
      "BUILD_OPTIMIZATION": "true",
      "NODE_ENV": "production",
      "NEXT_TELEMETRY_DISABLED": "1",
      "BUILD_VERSION": "$VERCEL_GIT_COMMIT_SHA",
      "DEPLOYMENT_ENV": "$VERCEL_ENV",
      "FORCE_DOMAIN_UPDATE": "v9-CRITICAL-DOMAIN-REASSIGNMENT-D74AA5E"
    }
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    }
  ],
  "headers": [
    {
      "source": "/dashboard",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate, max-age=0"
        }
      ]
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}